{
  "name": "@gvhinks/epl-integration-tests",
  "version": "1.6.0",
  "description": "",
  "main": "dist/index.js",
  "repository": {
    "url": "https://github.com/ghinks/epl-ml-mono-repo.git",
    "type": "git"
  },
  "scripts": {
    "integration": "jest",
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@gvhinks/epl-base-model": "^1.6.0",
    "@gvhinks/epl-common-interfaces": "^1.6.0",
    "@gvhinks/epl-constants": "^1.6.0",
    "@gvhinks/epl-data-reader": "^1.6.0",
    "@gvhinks/epl-data-to-db": "^1.6.0",
    "@gvhinks/epl-season-forecast": "^1.6.0",
    "@gvhinks/epl-utilities": "^1.6.0"
  },
  "devDependencies": {
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "@babel/plugin-transform-regenerator": "7.4.5",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-typescript": "7.6.0",
    "@babel/register": "7.6.2",
    "@babel/runtime": "7.6.2",
    "@tensorflow/tfjs-node": "1.2.9",
    "@types/node": "10.14.20",
    "@typescript-eslint/eslint-plugin": "2.2.0",
    "@typescript-eslint/parser": "2.2.0",
    "babel-jest": "24.9.0",
    "babel-plugin-rewire": "1.2.0",
    "cross-env": "5.2.1",
    "eslint": "6.5.1",
    "jest": "24.9.0",
    "prettier": "1.17.1",
    "rimraf": "3.0.0",
    "ts-jest": "24.1.0",
    "typescript": "3.6.3"
  },
  "babel": {
    "include": [
      "src/**/*.ts"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "10.0.0"
          }
        }
      ],
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ],
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-runtime"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/indent": [
        "error",
        2
      ],
      "@typescript-eslint/ban-ts-ignore": [
        "off"
      ]
    },
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "node": true,
      "jest": true,
      "mocha": true
    }
  },
  "jest": {
    "verbose": true,
    "collectCoverage": false,
    "testMatch": [
      "**/*.test.ts",
      "**/*.test.js"
    ],
    "coverageReporters": [
      "text",
      "html"
    ],
    "testEnvironment": "node"
  }
}
